<?xml version="1.0"?>
<extension key="ca.sdda.dogregistry" type="module">
  <file>dogregistry</file>
  <name>SDDA Dog Registry</name>
  <description>This extension allows for the use of the dog registry specific to the SDDA</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Norm Dressler</author>
    <email>norm@sportingdetectiondogs.ca</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/cub69/ca.sdda.dogregistry</url>
    <url desc="Documentation">https://github.com/cub69/ca.sdda.dogregistry</url>
    <url desc="Support">https://github.com/cub69/ca.sdda.dogregistry</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2018-11-06</releaseDate>
  <version>2.5</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>4.7</ver>
  </compatibility>
  <comments>Adds dog registry to CiviCRM and all of the forms needed to create and manage dog entries</comments>
  <civix>
    <namespace>CRM/Dogregistry</namespace>
  </civix>
</extension>
